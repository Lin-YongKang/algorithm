{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.(js|ts)(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "docs",
        "recurse": true,
        "debug": true,
        "encoding": "utf8",
        "package": "package.json",
        "readme": "README.md"
    }
}
